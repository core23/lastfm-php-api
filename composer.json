{
  "name": "core23/lastfm-api",
  "type": "library",
  "description": "Last.fm webservice client for php",
  "keywords": [
    "last.fm",
    "api",
    "crawler",
    "webservice client"
  ],
  "homepage": "https://core23.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Gripp",
      "email": "mail@core23.de"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pcre": "*",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0",
    "symfony/css-selector": "^4.2 || ^5.0",
    "symfony/dom-crawler": "^4.2 || ^5.0"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.3",
    "localheinz/composer-normalize": "^1.3",
    "nyholm/psr7": "^1.0",
    "symfony/http-client": "^4.4 || ^5.0"
  },
  "suggest": {
    "symfony/http-client": "Symfony HTTP client implementation"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Core23\\LastFm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Core23\\LastFm\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer bin all install --ansi"
    ],
    "post-update-cmd": [
      "@composer bin all update --ansi"
    ]
  }
}
